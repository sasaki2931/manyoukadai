<%= form_with(model: @task.new,local: true) do |form| %>
    <div class="task_naem">
      <%= form.label :naeme %>
      <%= form.text_field :name %>
    </div>
    <div class="task_content">
      <%= form.label :content %>
      <%= form.text_field :content %>
    </div>
    <%= form.submit %>
<% end %>