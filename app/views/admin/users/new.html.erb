<h1>ユーザーを登録する</h1>

<%= form_with(model: [:admin,@user], local: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
  
    <%= f.label :email %>
    <%= f.email_field :email %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <%= f.label :管理者権限 %>
    <%= f.check_box :admin, {} , "true", "false" %>


  
    <%= f.submit "Create  account" %>
  <% end %>